{
  "name": "@hexlet/code",
  "version": "1.0.0",
  "description": "Page loader utility",
  "type": "module",
  "bin": {
    "page-loader": "bin/page-loader.js"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:debug": "set DEBUG=page-loader,nock.*,axios&& set DEBUG_COLORS=0&& set DEBUG_FD=1&& node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --silent=false",
    "test:debug:unix": "DEBUG=page-loader,nock.*,axios DEBUG_COLORS=0 DEBUG_FD=1 node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --silent=false",
    "lint": "eslint ."
  },
  "keywords": [
    "cli",
    "page-loader",
    "hexlet",
    "codica"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "1.1.3",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.0.0",
    "debug": "^4.4.3"
  },
  "devDependencies": {
    "axios-debug-log": "^1.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nock": "^13.5.4"
  }
}
